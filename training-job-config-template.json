{
    "TrainingJobName": "[algorithm_name]-[curr_datetime]",
    "AlgorithmSpecification": {
        "TrainingImage": "[account].dkr.ecr.[region].amazonaws.com/[algorithm_name]",
        "TrainingInputMode": "File",
        "EnableSageMakerMetricsTimeSeries": true
    },
    "RoleArn": "arn:aws:iam::[account]:role/[project_sub_name]-SagemakerRole",
    "InputDataConfig": [
        {
            "ChannelName": "[data_name]",
            "DataSource": {
                "S3DataSource": {
                    "S3DataType": "S3Prefix",
                    "S3Uri": "s3://[project_name].smtj.data/[data_name]",
                    "S3DataDistributionType": "FullyReplicated"
                }
            },
            "ContentType": "image/[im_extention]"
        }
    ],
    "OutputDataConfig": {
        "S3OutputPath": "s3://[project_name].smtj.output"
    },
    "ResourceConfig": {
        "InstanceType": "[instance_type]",
        "InstanceCount": 1,
        "VolumeSizeInGB": 75
     },
    "StoppingCondition": {
        "MaxRuntimeInSeconds": 86400
    },
    "Tags": [ 
        { 
           "Key": "Customer",
           "Value": "[customer]"
        },
        { 
            "Key": "Project",
            "Value": "[project_sub_name]"
        },
        { 
            "Key": "Service",
            "Value": "sagemaker-training"
        }
     ]
}