{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "s3:ListBucket"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::[project_name].smtj.data",
                "arn:aws:s3:::[project_name].smtj.output"
            ]
        },
        {
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::[project_name].smtj.data/*",
                "arn:aws:s3:::[project_name].smtj.output/*"
            ]
        }
    ]
}